<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SetCardView">
        <attr name="title" format="string" />
        <attr name="count" format="integer" />
        <attr name="setId" format="integer" />

        <attr name="isSelected" format="boolean" />
        <attr name="activation" format="enum">
            <enum name="active" value="2" />
            <enum name="inactive" value="1" />
            <enum name="archived" value="0" />
        </attr>

        <attr name="state" format="enum">
            <enum name="enterTitle" value="0" />
            <enum name="contracted" value="1" />
            <enum name="expanded" value="2" />
        </attr>
    </declare-styleable>
</resources>